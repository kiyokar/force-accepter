-- This script can be executed via loadstring

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Create ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = playerGui

-- Create Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 250, 0, 150)
frame.Position = UDim2.new(0.5, -125, 0.5, -75)
frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
frame.BorderSizePixel = 0
frame.Parent = screenGui

-- Add a UI corner for rounded edges
local uiCorner = Instance.new("UICorner")
uiCorner.CornerRadius = UDim.new(0, 10)
uiCorner.Parent = frame

-- Create close button
local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0, 20, 0, 20)
closeButton.Position = UDim2.new(1, -25, 0, 5)
closeButton.Text = "X"
closeButton.TextColor3 = Color3.new(1, 1, 1)
closeButton.BackgroundColor3 = Color3.new(1, 0, 0)
closeButton.BorderSizePixel = 0
closeButton.Parent = frame

closeButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
end)

-- Add a UI corner for the close button for rounded edges
local closeUiCorner = Instance.new("UICorner")
closeUiCorner.CornerRadius = UDim.new(0, 5)
closeUiCorner.Parent = closeButton

-- Function to create buttons
local function createButton(text, positionY)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 200, 0, 50)
    button.Position = UDim2.new(0.5, -100, 0, positionY)
    button.Text = text
    button.TextColor3 = Color3.new(1, 1, 1)
    button.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
    button.BorderSizePixel = 0
    button.Parent = frame

    -- Add a UI corner for the button for rounded edges
    local buttonUiCorner = Instance.new("UICorner")
    buttonUiCorner.CornerRadius = UDim.new(0, 10)
    buttonUiCorner.Parent = button

    return button
end

-- Create buttons
local button1 = createButton("Force Accept", 50)
local button2 = createButton("Lag", 110)

-- Button actions (placeholders)
button1.MouseButton1Click:Connect(function()
    print("Force Accept clicked")
end)

button2.MouseButton1Click:Connect(function()
    print("Lag clicked")
end)

-- Create a label for the creator credit
local creditLabel = Instance.new("TextLabel")
creditLabel.Size = UDim2.new(1, 0, 0, 20)
creditLabel.Position = UDim2.new(0, 0, 1, -20)
creditLabel.Text = "Created by FuseScripts"
creditLabel.TextColor3 = Color3.new(1, 1, 1)
creditLabel.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
creditLabel.BorderSizePixel = 0
creditLabel.Font = Enum.Font.SourceSans
creditLabel.TextScaled = true
creditLabel.Parent = frame
